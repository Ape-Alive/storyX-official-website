                            "errorMessage": {
                              "type": "string",
                              "description": "错误信息（调用失败时的错误详情）",
                              "nullable": true,
                              "example": "API rate limit exceeded"
                            },
                            "requestTime": {
                              "type": "string",
                              "format": "date-time",
                              "description": "请求时间（客户端发起AI调用请求的时间）",
                              "example": "2024-01-01T12:00:00Z"
                            },
                            "responseTime": {
                              "type": "string",
                              "format": "date-time",
                              "description": "响应时间（AI服务返回结果的时间，调用失败时可能为null）",
                              "nullable": true,
                              "example": "2024-01-01T12:00:01Z"
                            },
                            "duration": {
                              "type": "integer",
                              "description": "调用耗时（毫秒），从请求到响应的时间差",
                              "nullable": true,
                              "example": 1500
                            },
                            "deviceFingerprint": {
                              "type": "string",
                              "description": "设备指纹（用于设备识别和管理）",
                              "nullable": true,
                              "example": "device_hash_abc123"
                            },
                            "ipAddress": {
                              "type": "string",
                              "description": "请求IP地址",
                              "nullable": true,
                              "example": "192.168.1.1"
                            }
                          }
